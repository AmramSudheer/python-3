from turtle import Turtle,Screen
timmy = Turtle()
timmy.shape("turtle")
timmy.color("red")
timmy.forward(100)
timmy.left(90)
timmy.forward(100)
timmy.left(90)
timmy.forward(200)
timmy.right(90)
timmy.forward(100)
timmy.right(90)
timmy.forward(100)
timmy.right(90)
timmy.forward(300)
timmy.backward(100)
timmy.left(45)
timmy.forward(150)
